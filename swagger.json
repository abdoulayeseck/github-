{
    "swagger": "2.0",
    "info": {
      "version": "1.0.0", 
      "title": "SOLIDARITE",
      "description": "Ce API a bout but de donner des informations sur les offres de logement sociales en france",
      "license": {
        "name": "MIT",
        "url": "https://opensource.org/licenses/MIT"
      }
    },
    "host": "localhost:3000",
    "basePath": "/",
    "tags": [
      {
        "name": "Information sociale",
      "description": "API sur les logements sociales en france"
      }
    ],
    "schemes": ["http"],
    "consumes": ["application/json"],
    "produces": ["application/json"],

    "paths": {
        "/Logement": {
          "get": {
            "tags": ["Logement"],
            "summary": "Renseigne le nombre de logement sociale par region",
            "responses": {
              "200": {
                "description": "OK",
                "schema": {
                  "$ref": "#/definitions/Logement"
                }
              }
            }
          }
        },
        "/Logement/:region": {
            "get": {
              "tags": ["Logement/:region"],
              "parameters":"region",
              "summary": "Renseigne les offre de logement sociales en fonction de la region souhaite",
              "responses": {
                "300": {
                  "description": "OKK",
                  "schema": {
                    "$ref": "#/definitions/Logement/:region"
                  }
                }
              }
            }
          },
          "/ville/:nom_ville": {
            "get": {
              "tags": ["ville/:nom_ville"],
              "parameters":[{"nom_ville":"ville"}],
              "summary": "Renseigne les informations sur une ville, ces donn√©es proviennent du projet de nos collegues",
              "responses": {
                "300": {
                  "description": "OKK",
                  "schema": {
                    "$ref": "#/definitions/ville/:nom_ville"
                  }
                }
              }
            }
          },


          "/Prix_max": {
            "get": {
              "tags": ["Prix_max"],
              "summary": "Renseigne le prix maximum d'offre de logement sociales",
              "responses": {
                "300": {
                  "description": "OKK",
                  "schema": {
                    "$ref": "#/definitions/Prix_max"
                  }
                }
              }
            }
          },
          "/nombre_logement": {
            "get": {
              "tags": ["nombre_logement"],
              "summary": "Renseigne le nombre de logement sociale par region",
              "responses": {
                "300": {
                  "description": "OK",
                  "schema": {
                    "$ref": "#/definitions/nombre_logement"
                  }
                }
              }
            }
          },
          "/api/user/:id": {
            "get": {
              "tags": ["/api/user/:id"],
              "summary": "Permet de recuperer l'avis d'un utilisateur",
              "responses": {
                "300": {
                  "description": "OK",
                  "schema": {
                    "$ref": "#/definitions/api/user/:id"
                  }
                }
              }
            }
          },
          "/api/user": {
            "post": {
              "tags": ["Users"],
              "description": "Creation d'un commentaite",
              "parameters": [
                {
                  "name": "user",
                  "in": "body",
                  "description": "les commentaire creer par l'utilisateur",
                  "schema": {
                    "$ref": "#/definitions/api/user/:id"
                  }
                }
              ],
              "produces": ["application/json"],
              "responses": {
                "200": {
                  "description": "Nouveau commentaire",
                  "schema": {
                    "$ref": "#/definitions/api/user/:id"
                  }
                }
              }
            }
          }

    },

    
    "definitions": {
      "Logement": {
        "required": [  "nom_region",
        "rehabilitation_prix_derevient_m2",
        "construction_prixderevient_m2",
        "rehabilitation_prixderevient_logement",
        "construction_surface_moy_m2_su",
        "annee_financement",
        "rehabilitation_surface_moy_m2_su",
        "construction_prixderevient_logement"],
        "properties": {
          "nom_region": {
            "type": "string"
          },
          "construction_prixderevient_m2": {
            "type": "number"
          },
          "construction_prixderevient_logement": {
            "type": "number"
          }
        }
      },
      
      "Logement/:region": {
        "required": ["nom_region",
        "rehabilitation_prix_derevient_m2",
        "construction_prixderevient_m2",
        "rehabilitation_prixderevient_logement",
        "construction_surface_moy_m2_su",
        "annee_financement",
        "rehabilitation_surface_moy_m2_su",
        "construction_prixderevient_logement"],
        "params":["nom_region"],
        "properties": {
          "nom_region": {
            "type": "string"
          },
          "construction_prixderevient_m2": {
            "type": "number"
          },
          "construction_prixderevient_logement": {
            "type": "number"
          }
        }
      },

      "ville/:nom_ville": {
        "required": ["nom_ville",
        "convoit","commentaitre"
        ],
        "params":["nom_ville"],
        "properties": {
          "nom_ville": {
            "type": "string"
          },
          "convoit": {
            "type": "string"
          },
          "commentaites": {
            "type": "string"
          }
        }
      },

      "Prix_max": {
        "required": [
          "region",
          "annee_financement",
          "rehabilitation_prixderevient_m2_max",
          "construction_prixderevient_m2"
        ],
        "properties": {
          "region": {
            "type": "string"
          },
          "annee_financement": {
            "type": "string"
          },
          "rehabilitation_prixderevient_m2_max": {
            "type": "number"
          },
          "construction_prixderevient_m2_max": {
            "type": "number"
          }
        }
      },
      "nombre_logement": {
        "required": [
          "nom_region",
          "libelle_departement",
          "nbre_logements_re"
        ],
        "properties": {
          "region": {
            "type": "string"
          },
          "libelle_departement": {
            "type": "string"
          },
          "nbre_logements_re": {
            "type": "number"
          }
        }
      },
      "/api/user/:id": {
        "required": [
          "region",
          "avis"
        ],
        "properties": {
          "region": {
            "type": "string"
          },
          "avis": {
            "type": "string"
          }
          
        }
      }
    }

}